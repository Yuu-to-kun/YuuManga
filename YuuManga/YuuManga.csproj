  </ItemGroup>

  <ItemGroup>
    <Reference Include="YuuScrapeLib">
      <HintPath>..\..\YuuScrape\YuuScrapeLib\bin\Release\net7.0\publish\win-x64\YuuScrapeLib.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
